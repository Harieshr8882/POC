	partition 'Table C' = calculated
		mode: import
		source =
				
				SELECTCOLUMNS (
				    ADDCOLUMNS (
				        'Table_B',
				        "Formatted Value",
				        SWITCH (
				            RELATED('Table_A'[DataType]),
				            "Decimal",
				                VAR SelectedDecimal = RELATED('Table_A'[Decimal Places])
